group_by(income_decile, quarter) %>%
summarise(calls = n(), .groups = "drop") %>%
group_by(income_decile) %>%
summarise(
avg_calls_per_quarter = mean(calls),
total_calls           = sum(calls),
.groups = "drop"
)
write_csv(decile_counts, file.path(out_dir, "decile_call_counts.csv"))
