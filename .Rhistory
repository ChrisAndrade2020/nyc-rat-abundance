mean,
~quantile(.x, c(0.025, 0.975))
) %>%
rename(
estimate = mean,
lower    = `2.5%`,
upper    = `97.5%`
) %>%
mutate(
# extract the Stan index from variable name (e.g., "lambda[3]")
idx = as.integer(str_extract(variable, "(?<=\\[)\\d+(?=\\])")),
